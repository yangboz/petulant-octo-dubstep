<?xml version="1.0" encoding="utf-8"?>
<s:Skin xmlns:fx="http://ns.adobe.com/mxml/2009" 
		xmlns:s="library://ns.adobe.com/flex/spark" 
		xmlns:mx="library://ns.adobe.com/flex/mx" 
		width="12" height="12"
		visible.normal="false">
	<!-- host component -->
	<fx:Metadata>
		[HostComponent("com.vstyran.transform.controls.Anchor")]
	</fx:Metadata>
	
	<s:states>
		<s:State name="normal"/>
		<s:State name="active"/>
	</s:states>
	
	<s:Ellipse top="1" bottom="1" left="1" right="1">
		<s:stroke>
			<s:SolidColorStroke/>
		</s:stroke>
		<s:fill>
			<s:SolidColor color="#FFFFFF"/>
		</s:fill>
	</s:Ellipse>
	<s:Ellipse top="0" bottom="0" left="0" right="0">
		<s:stroke>
			<s:SolidColorStroke color="white"/>
		</s:stroke>
	</s:Ellipse>
	
	<s:Ellipse top="4" bottom="4" left="4" right="4" verticalCenter="0" horizontalCenter="0">
		<s:fill>
			<s:SolidColor/>
		</s:fill>
	</s:Ellipse>
</s:Skin>
